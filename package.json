{
  "name": "visa-models",
  "version": "1.0.0",
  "description": "A shared models package for visatopia",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "keywords": [],
  "author": {
    "name": "Chris Oh",
    "email": "chris@5tigerjelly.com"
  },
  "repository": {
    "url": "https://github.com/5tigerjelly/visa-models"
  },
  "license": "ISC",
  "stability": "stable",
  "types": "company.ts",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "short",
    "targets": {
      "java": {
        "package": "fyi.visatopia.models",
        "maven": {
          "groupId": "fyi.visatopia.models",
          "artifactId": "core"
        }
      },
      "python": {
        "distName": "visatopia-models",
        "module": "visatopia_models.core"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "gts": "^3.1.0",
    "typescript": "^4.0.3"
  }
}
